package in.cmc.ui;
 class   A {
        public void show() {
            System.out.println("A - show");
        }
    }

    class   B  extends  Test {
        public void print() {
            System.out.println("B - print");
        }
    }

    public class   Test {
        public static void main(String[] args) {
            B obj = new B();
            obj.show();         // A - show
            obj.print();        // B - print

            Test obj2 = new Test();
            obj2.show();        // A - show
            obj2.print();       // error


            Test obj3 = new B();   
            obj3.show();        // A - show
            obj3.print();       // B - print
        }
    }